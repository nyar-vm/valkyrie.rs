extends<T, E> Result<T, E>: Perform {
	perform(self) -> T / E {
		match self {
			Success(s) => s
			Failure(e) => raise e
		}
	}
}

extends<T> Option<T>: Perform {
	perform(self) -> T / null {
		match self {
            Some(s) => s
			None => raise null
		}
	}
}

